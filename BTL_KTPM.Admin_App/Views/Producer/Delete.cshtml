@model BTL_KTPM.Application.Catalog.Producers.Dtos.DeleteProducerRequest

@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="container-fluid px-4">
    <h1 class="mt-4">Xóa danh mục sản phẩm</h1>
    <ol class="breadcrumb mb-4">
        <li class="breadcrumb-item"><a asp-action="Index" asp-controller="Home">Trang chủ</a></li>
        <li class="breadcrumb-item active">Xóa danh mục sản phẩm </li>
    </ol>
    <div style="margin: 20px 0;">
        <a class="btn btn-success" asp-action="Index" asp-controller="Producer">Danh sách nhà cung cấp</a>
    </div>
    <div class="card mb-4">
        <div class="card-body">
            <div class="row">
                
                <dd class="col-sm-10">
                  Bạn có muốn xóa nhà cung cấp có Id:  @Html.DisplayFor(model => model.Id) không ?
                </dd>

                <form asp-action="Delete">
                    <input asp-for="Id" type="hidden" />
                    <input type="submit" value="Xóa " class="btn btn-danger" />
                </form>
            </div>
        </div>

    </div>
</div>


@model BTL_KTPM.Data.entities.Contact

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="container-fluid px-4">
    <h1 class="mt-4">Thông tin phản hồi</h1>
    <ol class="breadcrumb mb-4">
        <li class="breadcrumb-item"><a asp-action="Index" asp-controller="Home">Trang chủ</a></li>
        <li class="breadcrumb-item active">Thông tin phản hồi </li>
    </ol>
    <div style="margin: 20px 0;">
        <a class="btn btn-success" asp-action="Index" asp-controller="Contact">Danh sách phản hồi</a>
    </div>
    <div class="card mb-4">
        <div class="card-body">
            <div class="row">
                <div class="col-md-12">
                    <dl class="row">

                        <dt class="col-sm-2">
                            Người gửi:
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.Name)
                        </dd>
                        <dt class="col-sm-2">
                            Email:
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.Email)
                        </dd>
                        <dt class="col-sm-2">
                            Tiêu đề
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.NumberPhone)
                        </dd>
                        <dt class="col-sm-2">
                            Nội dung
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.Message)
                        </dd>
                    </dl>
                    <div>
                    </div>
                </div>
            </div>

        </div>
    </div>
   
